  <%- include("header") -%>

  <div class="btnCon">
    <a href="/addTrans">+/-</a>
  </div>

  <div class="allTrans">
    <% for(tran of trans){%>
      <div class="tranCon">
        <div class="leftCon">
          <% if(tran.type == 'in'){%>
            <span class="inAmo">Rs.<%= tran.amount%></span>
          <%}else{%>
            <span class="exAmo">Rs.<%= tran.amount%></span>
          <%}%>
          <span><%= tran.date%></span>
        </div>
        <div class="rightCon">
          <span><%= tran.des%></span>
        </div>
      </div>
    <%}%>
    </div>
  </body>
</html>
